{
  "private": true,
  "name": "guess-the-1ink",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "build:client": "cd src/client && vite build",
    "build:server": "esbuild src/server/index.ts --bundle --platform=node --target=node18 --format=cjs --outfile=dist/server/index.cjs --external:@supabase/supabase-js",
    "build": "npm run build:client && npm run build:server",
    "dev:client": "cd src/client && vite",
    "dev:server": "tsx watch src/server/index.ts",
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "playtest": "dotenv -e .env -- devvit playtest",
    "login": "devvit login",
    "launch": "devvit publish",
    "type-check": "tsc --build",
    "predeploy": "npx ts-node scripts/predeploy.ts",
    "deploy": "npm run predeploy && devvit upload"
  },
  "dependencies": {
    "@devvit/mcp": "^0.0.21",
    "@devvit/public-api": "^0.12.10",
    "@devvit/web": "^0.12.10",
    "@supabase/supabase-js": "^2.89.0",
    "@types/express-session": "^1.18.2",
    "clsx": "^2.1.1",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "devvit": "^0.12.10",
    "dotenv-cli": "^11.0.0",
    "esbuild": "^0.27.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  }
}
